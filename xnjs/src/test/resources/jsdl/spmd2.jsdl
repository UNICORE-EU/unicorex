<?xml version="1.0"?>
<jsdl:JobDefinition xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl"
xmlns:jsdl-posix="http://schemas.ggf.org/jsdl/2005/11/jsdl-posix"
xmlns:jsdl-spmd="http://schemas.ogf.org/jsdl/2007/02/jsdl-spmd">
 <jsdl:JobDescription>
 <jsdl:Application>
  <jsdl-spmd:SPMDApplication>
     <jsdl-posix:Executable>/bin/date</jsdl-posix:Executable>
     <jsdl-posix:Input>stdin</jsdl-posix:Input>
     <jsdl-posix:Output>stdout</jsdl-posix:Output>
     <jsdl-posix:Error>stderr</jsdl-posix:Error>
     <jsdl-posix:Argument>-v</jsdl-posix:Argument>
     <jsdl-posix:Environment name="test">test_value</jsdl-posix:Environment>
     <jsdl-posix:Environment name="foo">bar</jsdl-posix:Environment>
     <jsdl-spmd:SPMDVariation>http://www.ogf.org/jsdl/2007/02/jsdl-spmd/OpenMPI</jsdl-spmd:SPMDVariation>
     <jsdl-spmd:NumberOfProcesses actualTotalCPUCount="true"/>
     <jsdl-spmd:ProcessesPerHost>4</jsdl-spmd:ProcessesPerHost>
     <jsdl-spmd:ThreadsPerProcess>2</jsdl-spmd:ThreadsPerProcess>
  </jsdl-spmd:SPMDApplication>
  </jsdl:Application>
 </jsdl:JobDescription>
</jsdl:JobDefinition>
