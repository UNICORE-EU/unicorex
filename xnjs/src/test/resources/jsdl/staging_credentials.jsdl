<?xml version="1.0"?>
<JobDefinition xmlns="http://schemas.ggf.org/jsdl/2005/11/jsdl">
  <JobDescription>
    <DataStaging>
      <FileName>foo.txt</FileName>
      <CreationFlag>overwrite</CreationFlag>
      <Source>
        <URI>http://some.where/foo.txt</URI>
      </Source>
      <Credential xmlns="http://schemas.ogf.org/hpcp/2007/01/fs">
        <UsernameToken xmlns="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
          <Username>user</Username>
          <Password>pass</Password>
        </UsernameToken>
      </Credential>
    </DataStaging>
    <DataStaging>
      <FileName>bar.txt</FileName>
      <CreationFlag>overwrite</CreationFlag>
      <Source>
        <URI>http://some.where/bar.txt</URI>
      </Source>
      <Credential xmlns="http://schemas.ogf.org/hpcp/2007/01/fs">
        <BearerToken xmlns="http://www.unicore.eu/unicore/jsdl-extensions">123</BearerToken>
      </Credential>
    </DataStaging>
    <DataStaging>
      <FileName>spam.txt</FileName>
      <CreationFlag>overwrite</CreationFlag>
      <Source>
        <URI>http://some.where/spam.txt</URI>
      </Source>
      <Credential xmlns="http://schemas.ogf.org/hpcp/2007/01/fs">
        <BearerToken xmlns="http://www.unicore.eu/unicore/jsdl-extensions"/>
      </Credential>
    </DataStaging>
  </JobDescription>
</JobDefinition>
